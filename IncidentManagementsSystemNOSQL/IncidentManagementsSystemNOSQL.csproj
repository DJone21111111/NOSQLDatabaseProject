    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Views\Account\NewFolder\**" />
    <Compile Remove="Views\DashboardViewModel\**" />
